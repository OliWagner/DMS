<Window x:Class="WpfApp.AendernTabelleDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp"
        mc:Ignorable="d"
        Title="Werte festlegen" Height="110" Width="330">
    <Window.CommandBindings>
    
    </Window.CommandBindings>

    <Grid x:Name="MeMyself">
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="25"></RowDefinition>
        </Grid.RowDefinitions>
       
        <Button x:Name="btnOk" Width="55" FontSize="10" Content="OK" Margin="05,5,0,5" Grid.Row="1" Click="btnOk_Click" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Button x:Name="btnAbbrechen" Width="55" FontSize="10" Content="Abbrechen" Margin="65,5,0,5" Grid.Row="1" Click="btnAbbrechen_Click" HorizontalAlignment="Left" VerticalAlignment="Center"/>
     </Grid>
</Window>
