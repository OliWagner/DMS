<UserControl x:Class="DMS_Adminitration.Dropfeld"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DMS_Adminitration"
        mc:Ignorable="d">

    <Grid MinWidth="200" AllowDrop="True" x:Name="grdDropzone" Margin="0,0,0,0" HorizontalAlignment="Center" DragDrop.Drop="Grid_Drop" VerticalAlignment="Center">
        <TextBlock TextWrapping="Wrap" x:Name="txtDropzone"  Margin="0,0,0,0" HorizontalAlignment="Center" Text="Dropzone" VerticalAlignment="Center" />
    </Grid>
</UserControl>
