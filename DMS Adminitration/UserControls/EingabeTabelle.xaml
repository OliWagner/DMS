<UserControl x:Class="DMS_Adminitration.EingabeTabelle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DMS_Adminitration"
             mc:Ignorable="d">

    <Grid HorizontalAlignment="Center" VerticalAlignment="Top">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <!--<RowDefinition Height="50" />-->
        </Grid.RowDefinitions>
        <ScrollViewer HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Visible" Grid.Row="0">
            <Grid x:Name="grdMain" Grid.Row="0" Margin="0,0,0,0" VerticalAlignment="Top" />
        </ScrollViewer>
        <!--<Grid Grid.Row="1" Height="50">
            <TextBox x:Name="txtTabellenname" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="250" ToolTip="Geben Sie hier den Tabellennamen ein."/>
        </Grid>-->
    </Grid>
    
</UserControl>
